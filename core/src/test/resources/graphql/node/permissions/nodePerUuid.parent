query Test($type: NodeContainerType) {
    
  # Via UUID
  nodePerUuid: node(uuid: "02e3982b6ad14c1fa4a0b5998a02ff92", nodeType: $type) {
    uuid
    version
    isPublished
    isDraft
    path

    # Via Parent
    parent(nodeType: $type) {
      uuid
      version
      isPublished
      isDraft
    }
  }
}

