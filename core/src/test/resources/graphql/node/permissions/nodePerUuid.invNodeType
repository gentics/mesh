query Test($type: NodeContainerType, $invType: NodeContainerType) {

  # Via UUID
  nodePerUuid: node(uuid: "02e3982b6ad14c1fa4a0b5998a02ff92", nodeType: $type) {

    # Inverted type
    invNodeType: node(nodeType: $invType) {
      uuid
      version
      isPublished
      isDraft
    }
  }
}

