{
  endsWithGroup: groups(filter:{
    name: { like: "group%"}
  }) {
    # [$.data.endsWithGroup.totalCount=2]
    totalCount
    elements {
      name
      uuid
    }
  }

  endsWithGroupAndAdmin: groups(filter:{
    or: [
    	{name: { equals:"admin"}}
    	{name: { like: "group%"}}
    ]
  }) {
    # [$.data.endsWithGroupAndAdmin.totalCount=3]
    totalCount
    elements {
      name
      uuid
    }
  }
}
# [$.errors=<is-undefined>]