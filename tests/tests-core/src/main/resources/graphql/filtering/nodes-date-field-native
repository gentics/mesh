{
	dateEquals: nodes(filter: {
	    schema: {is: folder}
	    fields: {
	      folder: {
	        date: {equals: "2012-07-11T08:55:21Z"}
	      }
	    }
	  }) {
	    # [$.data.dateEquals.elements.length()=1]
	    elements {
	      uuid
	      schema {
	        name
	      }
	    }
	}
	
	dateNotEquals: nodes(filter: {
	    schema: {is: folder}
	    fields: {
	      folder: {
	        date: {ne: "2013-07-11T08:55:21Z"}
