{
    branch {
		# [$.data.branch.uuid=<is-uuid>]
		uuid
		# [$.data.branch.name=dummy]
		name
		# [$.data.branch.latest=true]
		latest
		# [$.data.branch.migrated=true]
		migrated
		# [$.data.branch.hostname=getmesh.io]
		hostname
		# [$.data.branch.ssl=false]
		ssl
		# [$.data.branch.pathPrefix=/base/path]
		pathPrefix
		tags {
			# [$.data.branch.tags.totalCount=0]
			totalCount
		}
		schema(name:"content") {
            # [$.data.branch.schema.uuid=<is-uuid>]
            uuid
            # [$.data.branch.schema.name=content]
            name
		}
		uuidSchema: schema(uuid:"%SCHEMA_UUID%") {
		    # [$.data.branch.uuidSchema.uuid=%SCHEMA_UUID%]
		    uuid
		    # [$.data.branch.uuidSchema.name=folder]
		    name
		}
		schemas {
		    elements {
                # [$.data.branch.schemas.elements[0].uuid=<is-uuid>]
		        uuid
		    }
		}
    }
}
# [$.errors=<is-undefined>]