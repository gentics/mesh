    displayName: /{project}/navroot
    description: Provides an endpoint which can be used to retrieve a navigation response
    /{path}: 
        uriParameters: 
            path: 
                displayName: path
                description: Webroot path to the node language variation.
                type: string
                required: true
                repeat: false
                example: someFolder/somePage.html
