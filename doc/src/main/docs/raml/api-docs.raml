                                      "uuid" : "4a6892d1a49e4337a892d1a49e2337ae"
                                    },
                                    "edited" : "2018-07-25T12:05:00.884Z",
                                    "lastname" : "Doe",
                                    "firstname" : "Joe",
                                    "username" : "jdoe",
                                    "emailAddress" : "j.doe@nowhere.com",
                                    "nodeReference" : {
                                      "projectName" : "dummy",
                                      "uuid" : "7268c3bf1cb44987a8c3bf1cb4d987fc"
                                    },
                                    "enabled" : true,
                                    "groups" : [ {
                                      "name" : "editors",
                                      "uuid" : "ac7b1052cf2b4a16bb1052cf2bda16df"
                                    } ],
                                    "permissions" : {
                                      "create" : true,
                                      "read" : true,
                                      "update" : true,
                                      "delete" : true,
                                      "publish" : false,
                                      "readPublished" : false
                                    }
                                  }, {
                                    "uuid" : "25ef0579530e41a5af0579530e61a502",
                                    "creator" : {
                                      "firstName" : "Joe",
                                      "lastName" : "Doe",
                                      "uuid" : "c3e0d9a06d204a89a0d9a06d202a8990"
                                    },
                                    "created" : "2018-07-25T12:05:00.884Z",
                                    "editor" : {
                                      "firstName" : "Joe",
