            description: Return a navigation for the node which is located using the given path.
            queryParameters: 
                maxDepth: 
                    description: Specifies the maximum depth for the requested navigation tree structure.
                    type: number
                    required: false
                    repeat: false
                    default: "10"
                    example: "5"
