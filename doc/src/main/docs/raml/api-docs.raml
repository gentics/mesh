                                    "nodeReference" : {
                                      "projectName" : "dummy",
                                      "uuid" : "37c90ef8cddf4b59890ef8cddf0b5938"
                                    },
                                    "enabled" : true,
                                    "groups" : [ {
                                      "name" : "editors",
                                      "uuid" : "e5b7a0e60abd4925b7a0e60abdd92592"
                                    }, {
                                      "name" : "super-editors",
                                      "uuid" : "e66d66f486554d50ad66f486556d500e"
                                    }, {
                                      "name" : "editors",
                                      "uuid" : "35c4ace96c37477884ace96c37477823"
                                    } ],
                                    "permissions" : {
                                      "create" : true,
                                      "read" : true,
                                      "update" : true,
